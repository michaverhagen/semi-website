{
  "name": "semi-website",
  "version": "1.0.0",
  "description": "## Bundler is used",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "html-minifier": "^3.5.20",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prettier": "1.16.4",
    "sass-loader": "^7.1.0",
    "scriptjs": "2.5.9",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:html": "htmlproofer --url-ignore \"/www.semi.technology/\" --empty_alt_ignore ./_site",
    "prelocal": "npm run build",
    "local": "JEKYLL_ENV=develop bundle exec jekyll serve --livereload --config \"_config.yml\"",
    "htmlmin": "html-minifier --input-dir ./_site --output-dir ./_site --collapse-whitespace --file-ext html",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development --display-error-details",
    "build:watch": "webpack --mode=development --display-error-details --watch",
    "prettier": "prettier --check ./_js-src/modules/**",
    "eslint": "eslint ./_js-src/modules/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeMI-network/semi-website.git"
  },
